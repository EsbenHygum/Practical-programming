set term svg
set out 'plt.svg'
set terminal svg enhanced background rgb 'white0.5'
set xlabel 'x'
set ylabel 'y'
set grid
set key bottom right
plot \
	"diff_out.txt" using 1:2 lw 3 lc rgb '#000004' with lines title 'My result'\
	, 'diff_out.txt' using 1:3 dt "--.." lw 3 lc rgb '#4dbeee' with lines title 'Expected result'

